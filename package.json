{
  "name": "init-mate",
  "publisher": "SotirisSotiriou",
  "displayName": "InitMate",
  "description": "Helps you initialize all needed parameters based on the selected method's params",
  "license": "MIT",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SotiriouSotiris/init_mate"
  },
  "keywords": ["ruby", "initialize", "params", "parameters"],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "init-mate.addParameters",
        "title": "InitMate: Add Parameters"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.8.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
